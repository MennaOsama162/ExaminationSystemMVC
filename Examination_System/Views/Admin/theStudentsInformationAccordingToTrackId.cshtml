
@model IEnumerable<Examination_System.Models.Student>

@{
    Layout = null;
}


<h2>Students Information According to Track ID</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Address</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.Age</td>
                <td>@student.Address</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </tbody>
</table>
